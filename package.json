    "@playwright/test": "^1.54.1",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@swc/core": "^1.13.1",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
